{
  "manifest_version": 2,
  "content_security_policy": "default-src 'none'; style-src 'unsafe-inline'; script-src 'unsafe-inline'",
  "name": "GTrello",
  "description": "This extension allow you to quickly transform a mail into a Trello cards",
  "version": "1.0",
  "background": {
    "scripts": ["assets/js/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "js": ["assets/js/jquery-2.1.4.min.js","assets/js/gtrello.js"]
    /*"js": ["https://api.trello.com/1/client.js?key=8ddd1cdc81a0c0f22b1d176ce577a323"]
      "js": ["assets/js/trelloClient.js?key=8ddd1cdc81a0c0f22b1d176ce577a323"]*/
    }
  ],
  "page_action": {
    "default_name": "Display Map",
    "default_icon": "assets/images/icon.png",
    "default_popup": "assets/html/popup.html"
  },
  "permissions": [
    "tabs",
    "https://mail.google.com/*",
    "https://developers.google.com/maps/*",
    "declarativeContent"
  ]
}